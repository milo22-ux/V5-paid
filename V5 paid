    if (debug.info(3,"l") == 389 and debug.info(3,"s"):find("Camera")) then
        setstack(3, 1, {cameraXShake=0,rotSpeed=0,rotMag=0,returnTime=0,push=0,returnLerp=0,cameraY=0,cameraX=0,lerp=0})
    end
    return no_recoil(...)
end))
end)
